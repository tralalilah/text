    "keywords": ["string", "manipulate"],
    "homepage": "https://lilahsturges.com",
    "time": "2021-02-11",
    "license": "MIT",
    "authors": [
        {
            "name": "Lilah Sturges",
            "email": "lilah.sturges@gmail.com",
